{
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3.10.1",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.2",
        "firebase/php-jwt": "^6.11.1",
        "guzzlehttp/guzzle": "^7.9.3",
        "phpdocumentor/reflection-docblock": "^5.6.2",
        "phpstan/phpdoc-parser": "^2.2",
        "spatie/ray": "^1.42",
        "symfony/amqp-messenger": "~7.3.2",
        "symfony/asset": "~7.3.0",
        "symfony/asset-mapper": "~7.3.2",
        "symfony/console": "~7.3.2",
        "symfony/doctrine-messenger": "~7.3.2",
        "symfony/dotenv": "~7.3.2",
        "symfony/expression-language": "~7.3.2",
        "symfony/flex": "^2.8.1",
        "symfony/form": "~7.3.2",
        "symfony/framework-bundle": "~7.3.2",
        "symfony/http-client": "~7.3.2",
        "symfony/intl": "~7.3.2",
        "symfony/mailer": "~7.3.2",
        "symfony/messenger": "~7.3.2",
        "symfony/mime": "~7.3.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "~7.3.0",
        "symfony/process": "~7.3.0",
        "symfony/property-access": "~7.3.2",
        "symfony/property-info": "~7.3.1",
        "symfony/runtime": "~7.3.1",
        "symfony/security-bundle": "~7.3.2",
        "symfony/serializer": "~7.3.2",
        "symfony/stimulus-bundle": "^2.29.1",
        "symfony/string": "~7.3.2",
        "symfony/translation": "~7.3.2",
        "symfony/twig-bundle": "~7.3.2",
        "symfony/ux-turbo": "^2.29.1",
        "symfony/validator": "~7.3.2",
        "symfony/web-link": "~7.3.0",
        "symfony/yaml": "~7.3.2",
        "twig/extra-bundle": "^2.12 || ^3.21",
        "twig/twig": "^2.12 || ^3.21.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.3",
        "ergebnis/composer-normalize": "^2.47",
        "phpstan/phpstan": "^2.1.22",
        "phpstan/phpstan-symfony": "^2.0.7",
        "phpunit/phpunit": "^12.3.4",
        "phpyh/coding-standard": "^3.0",
        "phpyh/service-dumper-bundle": "^0.3.4.6",
        "rector/rector": "^2.1.3",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "~7.3.0",
        "symfony/debug-bundle": "~7.3.0",
        "symfony/maker-bundle": "^1.64",
        "symfony/phpunit-bridge": "^7.3",
        "symfony/stopwatch": "~7.3.0",
        "symfony/web-profiler-bundle": "~7.3.2"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
